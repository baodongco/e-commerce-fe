{
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaFeatures": {
      "globalReturn": false
    }
    // "babelOptions": {
    //   "configFile": "path/to/config.js",
    // },
  },
  "settings": {
    "react": {
        "version": "latest"
    }
  },
  "env": {
    "browser": true,
    "node": true
  }
}
